
<form method="POST" action="/vets/<%= @vet.id %>">
  <label for="name-field">Name:</label>
  <input
  type="text"
  name="name"
  id="name-field"
  value="<%= @vet.name %>"
  />

  <label for="specialism-field">Specialism:</label>
  <input
  type="text"
  name="specialism"
  id="specialism-field"
  value="<%= @vet.specialism %>"
  />

  <input type='submit' value="Edit Vet">
</form>
